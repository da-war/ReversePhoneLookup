{
  "expo": {
    "name": "Reverse-Phone-Lookup",
    "slug": "Reverse-Phone-Lookup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "439b93ce-e21f-4bc6-bb3e-93333426bf6f"
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      },
      "bundleIdentifier": "Com.Number.Book.Reverse.Phone.Lookup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-build-properties", "expo-localization"]
  }
}
